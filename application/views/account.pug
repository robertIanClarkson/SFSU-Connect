extends layout

block content
    //- Square cards

    .container
 
      

      .row
      .card-panel.deep-purple
        form.col.s12
          .row
            .input-field.col.s6
              span.white-text.text-darken-2 My Account
         

    .container
 
      
      .row
      .card-panel.deep-purple
        form.col.s12
          .row
            .input-field
              span.white-text.text-darken-2 My Information
            .input-field
              span.white-text.text-darken-2 First Name:
              input#first_name2.validate(type='text')
            .input-field
                span.white-text.text-darken-2 Last Name:
                input#last_name2.validate(type='text')
            .input-field
              span.white-text.text-darken-2 Email
              input#email_name2.validate(type='text')

      .container
 
      
      .row
      .card-panel.deep-purple
        form.col.s12
          .row
            .input-field
              span.white-text.text-darken-2 Manage Items
            .input-field
                span.white-text.text-darken-2 Inbox
                
      .container     

      .row
        .center-align
          img.responsive-img.circle(src='../images/profile_pics/taylor.png' alt='Image Circle')
    
    
